{
  "name": "@amzn/amazon-connect-messaging-data-converter",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "repository": "ssh://git.amazon.com/pkg/AmazonConnectMessagingDataConverter",
  "homepage": "https://code.amazon.com/packages/AmazonConnectMessagingDataConverter",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "rollup -c",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest --collectCoverage --collectCoverageFrom=src/**/*.{ts,js} --env=jsdom",
    "posttest": "generate-coverage-data --language typescript",
    "format": "prettier --write 'src/**' 'test/**' package.json README.md rollup.config.js tsconfig.json",
    "pretest": "prettier --check 'src/**' 'test/**' package.json README.md rollup.config.js tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jest": {
    "testMatch": [
      "**/**.test.ts"
    ],
    "transform": {
      ".(js|ts)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx|json)$",
      "package.json"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ]
  },
  "files": [
    "dist"
  ],
  "npm-pretty-much": {
    "runTest": "never"
  },
  "devDependencies": {
    "@amzn/brazil": "^2.0.5",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.2.5",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-copy": "^3.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "aws-sdk": "^2.1618.0"
  }
}
