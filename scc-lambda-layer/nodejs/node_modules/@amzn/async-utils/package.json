{
  "name": "@amzn/async-utils",
  "version": "2.4.0",
  "description": "",
  "contributors": [
    "dschontz@amazon.com",
    "hanfang@amazon.com",
    "justdar@amazon.com"
  ],
  "license": "UNLICENSED",
  "source": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "repository": "ssh://git.amazon.com/pkg/AsyncUtilsTS",
  "homepage": "https://code.amazon.com/packages/AsyncUtilsTS",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:dist",
    "clean:build": "rm -rf build/*",
    "clean:dist": "rm -rf dist",
    "test": "vitest run",
    "build": "vite build && tsc",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "release": "npm run build",
    "examples": "tsc -p tsconfig.examples.json"
  },
  "npm-pretty-much": {
    "runTest": "never",
    "ciBuild": "always"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "prettier": "^2.5.1",
    "typescript": "^4.9.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "dependencies": {}
}
